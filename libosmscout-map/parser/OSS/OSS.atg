  STYLEFILTER<StyleFilter& filter>
              = "["
                   [
                     (.
                       TypeSet     types;
                       std::string name;
                     .)

                     "TYPE"
                     IDENT<name>
                     (.
                       TypeId type=config.GetTypeConfig()->GetTypeId(name);

                       if (type==typeIgnore) {
                         std::string e="Unknown type '"+name+"'";

                         SemErr(e.c_str());
                       }
                       else if (filter.HasTypes() &&
                                !filter.HasType(type)) {
                         std::string e="Type '"+name+"' is not included by parent filter";

                         SemErr(e.c_str());
                       }
                       else {
                         types.SetType(type);
                       }
                     .)
                     {
                       (. std::string name; .)
                       ","
                       IDENT<name>
                       (.
                         TypeId      type=config.GetTypeConfig()->GetTypeId(name);

                         if (type==typeIgnore) {
                           std::string e="Unknown type '"+name+"'";

                           SemErr(e.c_str());
                         }
                         else if (filter.HasTypes() &&
                                  !filter.HasType(type)) {
                           std::string e="Type '"+name+"' is not included by parent filter";

                           SemErr(e.c_str());
                         }
                         else {
                           types.SetType(type);
                         }
                       .)
                     }

                     (. filter.SetTypes(types); .)
                   ]
