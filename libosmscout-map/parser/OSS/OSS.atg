                       MAG<mag>
                       (.
                          size_t level=MagToLevel(mag);

                          if (level>filter.GetMaxLevel()) {
                           std::string e="The magnification interval end is not within the parent magnification range";

                           SemErr(e.c_str());
                          }
                          else {
                            filter.SetMaxLevel(level);
                          }
                       .)
                     ]
                   ]
                "]"
