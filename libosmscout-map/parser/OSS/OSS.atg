                       (. Mag mag; .)
                       MAG<mag>
                       (.
                          size_t level=MagToLevel(mag);

                          if (level<filter.GetMinLevel()) {
                           std::string e="The magnification interval start is not within the parent magnification range";

                           SemErr(e.c_str());
                          }
                          else {
                            filter.SetMinLevel(level);
                          }
                       .)
                     ]
