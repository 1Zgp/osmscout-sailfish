  TYPE highway_motorway
    = WAY ("highway"=="motorway")
      {Name, NameAlt, Ref}
      PATH[CAR] LOCATION OPTIMIZE_LOW_ZOOM
  TYPE highway_motorway_trunk
    = WAY ("highway"=="trunk" AND EXISTS "motorroad" AND !("motorroad" IN ["no","false","0"]))
