OSS
  AREA "boundary_administrative"
    FILL #cccccc BORDER #000000 WITH MINPIXEL 2
    LABEL WITH STYLE emphasize SIZE 1.5 MINMAG world PRIO 1
   
  NODE "place_city"
    LABEL WITH SIZE 1.3 MINMAG stateOver PRIO 2
    SYMBOL circle #ff0000aa 4 WITH MINMAG world

  NODE "place_town"
    LABEL WITH SIZE 1.2 MINMAG county PRIO 3
    SYMBOL circle #ff000055 4 WITH MINMAG county
END
