    AREA {color: #cccccc; borderColor: #000000; borderWidth: 0.5mm;}
    AREA.LABEL { style: emphasize; size: 1.5; priority: 1; }
  }  

  [TYPE "place_city" MAG stateOver-] {
	NODE.LABEL {style: normal; size: 1.3; priority: 2;}
