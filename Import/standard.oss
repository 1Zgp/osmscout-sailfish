 // -------------------------------------------------------
 //
 // natural_* (without coastline)
 //

 [MAG county-] {
   [TYPE "natural_glacier"] AREA { color: #ddecec; }
   [TYPE "natural_fell"] AREA { color: #f9f9f9; }
   [TYPE "natural_heath"] AREA { color: #ffffc0; }
   [TYPE "natural_wood"] AREA { color: #aed1a0; }
   [TYPE "natural_marsh", "natural_wetland_marsh"] AREA { color: #efe8ff; }
   [TYPE "natural_wetland", "natural_wetland_tidalflat"] AREA { color: #bdd8d8; }
   [TYPE "natural_water"] AREA { color: #b5d0d0; }
   [TYPE "natural_beach"] AREA { color: #eecc55; }
   [TYPE "natural_land"] AREA { color: #ffffff; }
   [TYPE "natural_scrub"] AREA { color: #b5e2b5; pattern: "scrub"; patternMinMag: cityOver; }
 }

 [MAG detail-] {
   [TYPE "natural_glacier",
         "natural_fell",
         "natural_heath",
         "natural_wood",
         "natural_scrub",
         "natural_marsh",
         "natural_wetland",
         "natural_water",
         "natural_beach",
         "natural_land"]
     AREA.LABEL { style: normal; }

   [TYPE "natural_peak"] NODE.LABEL { style: emphasize; color: #9c5329;}
 }

 [MAG city-] {
   [TYPE "natural_peak"] NODE.ICON { symbol: natural_peak; }
 }

 // -------------------------------------------------------
 //
 // waterway_*
 //

 [MAG proximity-] {
   [TYPE "waterway_river",
         "waterway_canal"]
     WAY {color: #b5d0d0; displayWidth: 0.1mm; width: 15m;}

   [TYPE "waterway_dock",
         "waterway_riverbank"] {
     AREA { color: #b5d0d0; }
     AREA.LABEL { style: normal;}
   }
 }

 [MAG cityOver-] {
   [TYPE "waterway_stream"] WAY {color: #b5d0d0; displayWidth: 0.25mm; width: 2m;}
   [TYPE "waterway_drain"] WAY {color: #b5d0d0; displayWidth: 0.1mm; width: 3m;}
 }

 [MAG block-] {
   [TYPE "waterway_stream",
         "waterway_drain",
         "waterway_river",
         "waterway_canal"]
     WAY.LABEL { style: contour;}

   [TYPE "waterway_riverbank"]
     AREA.LABEL { style: normal;}
 }

 // -------------------------------------------------------
 //
 // highway_*, railway_*, route_ferry
 //

 [TYPE "highway_motorway",
       "highway_motorway_link",
       "highway_trunk",
       "highway_trunk_link",
       "highway_primary",
       "highway_primary_link",
       "highway_secondary",
       "highway_secondary_link",
       "highway_tertiary",
       "highway_road",
       "highway_unclassified",
       "highway_residential",
       "highway_service",
       "highway_living_street",
       "highway_pedestrian"] WAY {outline: 0.25mm;}

 [MAG continent-] {
   [TYPE "highway_motorway"] WAY { color: #809bc0; outlineColor: #777777; displayWidth: 0.45mm; width: 20m;}
 }

 [MAG county-] {
   [TYPE "highway_trunk"] WAY { color: #a8daa8; outlineColor: #777777; displayWidth: 0.45mm; width: 18m;}
   [TYPE "highway_primary"] WAY { color: #ec989a; outlineColor: #a56769; displayWidth: 0.4mm; width: 14m; }
   [TYPE "highway_services"] AREA { color: #cb9999; }
 }

 [MAG proximity-] {
   [TYPE "highway_secondary"] WAY { color: #fdd6a4; outlineColor: #bfbfbf; altColor: #fdd654; displayWidth: 0.25mm; width: 12m; }
 }

 [MAG cityOver-] {
   [TYPE "highway_motorway_link"] WAY { color: #809bc0; outlineColor: #777777; displayWidth: 0.45mm; width: 20m; }
   [TYPE "highway_trunk_link"] WAY { color: #a8daa8; outlineColor: #777777; displayWidth: 0.45mm; width: 18m; }
   [TYPE "highway_tertiary"] WAY { color: #fefeb3; outlineColor: #bfbfbf; altColor: #fefe73; displayWidth: 0.25mm; width: 10m; }
   [TYPE "highway_road"] WAY { color: #ffffff; outlineColor: #bfbfbf; altColor: #888888; displayWidth: 0.1mm; width: 8m; }
   [TYPE "railway_rail"] WAY {color: #939393; altColor: #777777; outlineColor: #b3b3b3; dash: 3,3; cap: butt; gapColor: #ffffff; displayWidth: 0.25mm; width: 5m; outline: 0.25mm;}
 }

 [MAG city-] {
   [TYPE "highway_primary_link"] WAY { color: #ec989a; outlineColor: #777777; displayWidth: 0.4mm; width: 14m; }
   [TYPE "highway_secondary_link"] WAY { color: #fdd6a4; outlineColor: #777777; altColor: #fdd654; displayWidth: 0.25mm; width: 12m; }
   [TYPE "highway_unclassified",
         "highway_residential"] WAY { color: #ffffff; outlineColor: #bfbfbf; altColor: #999999; displayWidth: 0.1mm; width: 8m; }

   [TYPE "railway_tram"] WAY {color: #444444; displayWidth: 0.25mm; width: 5m;}
   [TYPE "railway_light_rail"] WAY {color: #b3b3b3; displayWidth: 0.25mm; width: 5m;}
   [TYPE "railway_subway"] WAY {color: #b3b3b3; outlineColor: #ffffff50; displayWidth: 0.25mm; width: 5m; outline: 0.25mm;}

   [TYPE "railway_station"] AREA { color: #cb4040; }

   [TYPE "route_ferry"] WAY {color: #0000ff; outlineColor: #ffffff50; dash: 1,1; cap: butt; displayWidth: 0.5mm; width: 2m; outline: 0.4mm;}
 }

 [MAG detail-] {
   [TYPE "highway_service"] WAY { color: #ffffff; outlineColor: #bfbfbf; altColor: #999999; displayWidth: 0.1mm; width: 8m; }
   [TYPE "highway_living_street"] WAY { color: #bababa; outlineColor: #bfbfbf; displayWidth: 0.25mm; width: 8m;}
   [TYPE "highway_bus_guideway"] WAY {color: #90570c; displayWidth: 0.1mm; width: 2m; }

   [TYPE "railway_disused"] WAY {color: #939393; dash: 1.5,1.5; cap: butt; displayWidth: 0.2mm; width: 4m;}
 }

 [MAG close-] {
   [TYPE "railway_station"] NODE.ICON { symbol: railway_station; }
   [TYPE "highway_bus_stop"] NODE.ICON { symbol: highway_bus_stop; name: bus_stop;}

   [TYPE "highway_construction"] WAY {color: #99cccc; displayWidth: 0.1mm; width: 2m;}

   [TYPE "highway_pedestrian"] {
     WAY {color: #ebeaea; displayWidth: 0.1mm; width: 8m; }
     AREA { color: #ededed; borderColor: #c5c5c5; borderWidth: 0.1mm; }
   }

   [TYPE "public_transport_platform"] {
     WAY {color: #9b9b9c; outlineColor: #887b7b; displayWidth: 0.75mm; width: 2m; outline: 0.1mm;}
     AREA { color: #9b9b9c; borderColor: #887b7b; borderWidth: 0.1mm; }
   }

   [TYPE "highway_service"] AREA { color: #ffffff; borderColor: #cccccc; borderWidth: 0.1mm; }
   [TYPE  "highway_footway"] AREA { color: #ededed; borderColor: #c5c5c5; borderWidth: 0.1mm; }
 }

 [MAG veryClose-] {
   [TYPE "highway_track"] WAY {color: #90570c; outlineColor: #ffffff50; dash: 3,1; cap: butt; displayWidth: 0.4mm;  outline: 0.4mm;}
   [TYPE "highway_pedestrian"] WAY {color: #ebeaea; outlineColor: #ffffff50; displayWidth: 0.1mm; outline: 0.25mm;}
   [TYPE "highway_path"] WAY {color: #555555; outlineColor: #ffffff50; dash: 8,2; cap: butt; displayWidth: 0.25mm; outline: 0.25mm;}
   [TYPE "highway_cycleway"] WAY {color: #4645fa; outlineColor: #ffffff50; dash: 1,1; cap: butt; displayWidth: 0.4mm; outline: 0.4mm; }
   [TYPE "highway_footway"] WAY {color: #f64762; outlineColor: #ffffff50; dash: 1,1; cap: butt; displayWidth: 0.4mm; outline: 0.4mm; }
   [TYPE "highway_bridleway"] WAY {color: #90570c; displayWidth: 0.1mm; width: 2m; }
  }

 [MAG block-] {
   [TYPE "railway_halt"] NODE.ICON { symbol: railway_halt;}

   [TYPE "railway_tram_stop"] NODE.ICON { symbol: railway_tram_stop;}

   [TYPE "highway_byway"] WAY {color: #90570c; displayWidth: 0.1mm; width: 2m; }

   [TYPE "highway_steps"] WAY {color: #fe9897; dash: 0.25,0.25; cap: butt; displayWidth: 1.25mm;}
 }

