   [TYPE "leisure_nature_reserve"] AREA.LABEL { style: normal;}
 }

 [MAG close-]{
   [TYPE "leisure_sports_centre",
         "leisure_golf_course",
         "leisure_stadium",
         "leisure_track",
         "leisure_water_park",
         "leisure_marina",
         "leisure_fishing",
         "leisure_park",
         "leisure_playground",
         "leisure_garden",
         "leisure_common",
         "leisure_ice_rink",
         "military_airfield",
         "military_bunker",
         "military_barracks",
         "military_danger_area",
         "military_range",
         "military_naval_base"] AREA.LABEL { style: normal;}

   [TYPE "railway_station"] AREA.LABEL { style: emphasize; color: #8888fd;}
 }

 [MAG veryClose-] {
   [TYPE "amenity_hospital"] NODE.LABEL { style: emphasize; color: #da0092; size: 1.1; }

   [TYPE "christian_church_building",
         "christian_church"] {
     AREA.LABEL { style: emphasize; color: #000000;}
     AREA.ICON {symbol: christian_church_cross; }

     NODE.LABEL { style: emphasize; color: #000000;}
     NODE.ICON {symbol: christian_church_cross; }
   }

   [TYPE "leisure_pitch"] AREA.LABEL { style: normal;}

   [TYPE "amenity_hospital_building",
         "amenity_hospital"] AREA.LABEL { style: emphasize; color: #da0092; size: 1.1;}

   [TYPE "amenity_grave_yard"] AREA.LABEL { style: emphasize; color: #000000; size: 1.1;}
 }

 [MAG block-] {
   [TYPE "amenity_pharmacy"] NODE.LABEL { style: emphasize; color: #da0092; size: 1.1;}

   [TYPE "amenity",
         "tourism_attraction"] NODE.LABEL { style: emphasize;color: #000000; size: 1.1; }

   [TYPE "shop"] NODE.LABEL {style: normal; }

   [TYPE "amenity_parking"] AREA.LABEL { style: emphasize; color: #4bb2da; size: 1.1; }

   [TYPE "leisure_building",
         "sport_building",
         "shop"] AREA.LABEL { style: normal;}

   [TYPE "building"] AREA.LABEL { style: normal; color: #524a4a; size: 0.8; }

   [TYPE "power_station",
         "power_sub_station",
         "power_generator",
         "tourism_theme_park",
         "tourism_zoo",
         "historic_archaeological_site",
         "historic_battlefield",
         "tourism_alpine_hut",
         "tourism_attraction",
         "tourism_artwork",
         "tourism_camp_site",
         "tourism_caravan_site",
         "tourism_chalet",
         "tourism_guest_house",
         "tourism_hotel",
         "tourism_information",
         "tourism_motel",
         "tourism_museum",
         "tourism_picnic_site",
         "historic_castle",
         "historic_monument",
         "historic_memorial",
         "historic_ruins",
         "historic_wreck",
         "amenity_post_office_building",
         "amenity_post_office",
         "amenity_taxi",
         "amenity_building",
         "amenity"] AREA.LABEL { style: emphasize; color: #000000; size: 1.1; }

   [TYPE "amenity_parking",
         "amenity_parking_building"] AREA.LABEL { style: emphasize; color: #4bb2da; size: 1.1; }
 }


 // -------------------------------------------------------
 //
 // Places
 //

 [MAG world-city] {
   [TYPE "place_continent"] NODE.LABEL { style: emphasize; priority: 1; }
 }

 [MAG stateOver-city] {
   [TYPE "place_country"] NODE.LABEL { style: emphasize; priority: 2; }
 }

 [MAG state-county] {
   [TYPE "place_state"] NODE.LABEL { style: emphasize; size: 1.7; scaleMag: state; priority: 3; }
 }

 [MAG region-city] {
   [TYPE "place_region"] NODE.LABEL { style: emphasize; priority: 4; }
 }

 [MAG county-city] {
   [TYPE "place_county"] NODE.LABEL { style: emphasize; priority: 5;}
 }

 [MAG stateOver-city] {
   [TYPE "place_city"] NODE.LABEL { style: emphasize; size: 1.3; scaleMag: county; priority: 6;}
 }

 [MAG region-city] {
   [TYPE "place_town"] NODE.LABEL { style: emphasize; size: 1.2; scaleMag: region; priority: 7;}
 }

 [MAG region-veryClose] {
   [TYPE "place_island"] {
     NODE.LABEL { style: emphasize; size: 1.3; priority: 1;}
     AREA.LABEL { style: emphasize; size: 1.3; }
   }
 }

 [MAG cityOver-veryClose] {
   [TYPE "place_village"] NODE.LABEL { style: emphasize; size: 1.2; scaleMag: suburb; priority: 8;}
 }

 [MAG city-veryClose] {
   [TYPE "place_hamlet"] NODE.LABEL { style: emphasize; size: 1.0; scaleMag: city; priority: 9;}
}

 [MAG cityOver-veryClose] {
   [TYPE"place_suburb"] NODE.LABEL { style: emphasize; size: 1.3; scaleMag: city; priority: 10;}
 }

 [MAG close-] {
   [TYPE "place_locality"] NODE.LABEL { style: emphasize; size: 1.1; priority: 11;}
}

 // -------------------------------------------------------
 //
 // Aeroway
 //

 [MAG cityOver-] {
   [TYPE "aeroway_aerodrome",
         "aeroway_helipad"] AREA { color: #d5ddbd; }

   [TYPE "aeroway_terminal"] AREA { color: #cc99ff; }
   [TYPE "aeroway_runway"] AREA { color: #bbbbcc; }
   [TYPE "aeroway_apron"] AREA { color: #e8d1fe; }
 }

 [MAG suburb-] {
   [TYPE "aeroway_runway"] WAY {color: #bbbbcc; displayWidth: 0.5mm; width: 20m;}
   [TYPE "aeroway_taxiway"] WAY {color: #bbbbcc; displayWidth: 0.5mm; width: 15m;}
 }

 [MAG close-] {
   [TYPE "aeroway_taxiway",
         "aeroway_runway"] WAY.LABEL { style: contour;}

   [TYPE "aeroway_aerodrome",
         "aeroway_terminal",
         "aeroway_helipad",
         "aeroway_runway",
         "aeroway_apron"] AREA.LABEL{ style: normal;}
 }
END
